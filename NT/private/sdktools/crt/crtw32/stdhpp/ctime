// ctime standard header
#pragma once
#ifndef _CTIME_
#define _CTIME_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <time.h>
 #define _STD_USING
#else /* _STD_USING */
 #include <time.h>

_STD_BEGIN
using ::clock_t; using ::size_t;
using ::time_t; using ::tm;
using ::asctime; using ::clock; using ::ctime;
using ::difftime; using ::gmtime; using ::localtime;
using ::mktime; using ::strftime; using ::time;
_STD_END
#endif /* _STD_USING */
#endif /* _CTIME_ */


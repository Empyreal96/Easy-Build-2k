// cctype standard header
#pragma once
#ifndef _CCTYPE_
#define _CCTYPE_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <ctype.h>
 #define _STD_USING
#else /* _STD_USING */
 #include <ctype.h>

_STD_BEGIN
using ::isalnum; using ::isalpha; using ::iscntrl;
using ::isdigit; using ::isgraph; using ::islower;
using ::isprint; using ::ispunct; using ::isspace;
using ::isupper; using ::isxdigit; using ::tolower;
using ::toupper;
_STD_END
#endif /* _STD_USING */
#endif /* _CCTYPE_ */


#pragma once
#ifndef _CLOCALE_
#define _CLOCALE_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <locale.h>
 #define _STD_USING
#else /* _STD_USING */
 #include <locale.h>

_STD_BEGIN
using ::lconv; using ::localeconv; using ::setlocale;
_STD_END
#endif /* _STD_USING */
#endif /* _CLOCALE_ */


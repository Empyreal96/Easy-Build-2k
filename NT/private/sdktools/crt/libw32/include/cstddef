#pragma once
#ifndef _CSTDDEF_
#define _CSTDDEF_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <stddef.h>
 #define _STD_USING
#else /* _STD_USING */
 #include <stddef.h>

_STD_BEGIN
using ::ptrdiff_t; using ::size_t;
_STD_END
#endif /* _STD_USING */
#endif /* _CSTDDEF_ */


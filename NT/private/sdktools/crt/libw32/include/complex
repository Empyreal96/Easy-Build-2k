		}

	static _Ty __cdecl _Infv(_Ty)
		{	// return infinity
		return (::_Inf._Double);
		}

	static bool __cdecl _Isinf(_Ty _Left)
		{	// test for infinity
		return (::_Dtest(&_Left) == _INFCODE);
		}

	static bool __cdecl _Isnan(_Ty _Left)
		{	// test for NaN
		return (::_Dtest(&_Left) == _NANCODE);
